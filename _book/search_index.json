ter=$struct[2];\n$pointer-&gt;[4]=&#39;e&#39;;\npush(@$pointer,&quot;f&quot;);\nprint &quot;@{$pointer}&quot;.&quot;\\n&quot;;\nͨ�����öԻ��Ͻṹ�Ĺ�ϣ���ݽ��в���\n$pointer=$struct[3];\n${$pointer}{e0}=&#39;15&#39;;\nforeach( sort keys %{$struct[3]}){\n    print &quot;$_ &quot;;\n}\nprint &quot;\\n&quot;;\nforeach( sort values %{$struct[3]}){\n    print &quot;$_ &quot;;\n}\n"},"chapter6/6_16.html":{"url":"chapter6/6_16.html","title":"6_16","body":"�����ʱ�\nsub ReadDict{\n    my ($file)=@_;\n    open(In,$file);\n    while(){\n        chomp;\n    #����ģʽƥ����ȡ�����еĵ��ʺ��������顣���ڲ�������ͬ��\n    #�������ƣ�������Ҫʹ�á�my�������µĵ�ַ�ռ�\n    if(/(.*)=&gt;(.*)/){\n        my $word=$1;\n        #�á�@����Ϊ��������ȡ�ö���������ŵ�������\n        #ע������������һ����my���ͱ���\n        my @trans=$2=~/[^@]+/g;\n        #����ֵΪ�����Ĺ�ϣ����\n        $hash{$word}=\\@trans;\n        }\n    }\nclose(In);\n}\n"},"chapter6/6_17.html":{"url":"chapter6/6_17.html","title":"6_17","body":"sub Search{\n    my($word)=@_;\n#�ڹ�ϣ������%hash�����ҵ���\nif(defined $hash{$word}){\n    #�����ҵ���ȡ��ָ����������������\n    $pointer=$hash{$word};\n    #ͨ�����ã�����������Ա\n    foreach(@$pointer){\n            print &quot;$_\\n&quot;;\n        }\n    }else{\n        print &quot;not found!\\n&quot;;    \n    }\n}\n"},"chapter6/6_18.html":{"url":"chapter6/6_18.html","title":"6_18","body":"�������ܣ����ʵ�\nʵ��˼·����ֵΪ�����Ĺ�ϣ��װ�ʵ�\n���ʵ�\nsub ReadDict{\n    my($file)=@_;\n    open(In,$file);\n    while(){\n        chomp;\n    #���õ��ʺ�����\n    if(/(.*)=&gt;(.*)/){\n        my $word=$1;\n        #��@��Ϊ���룬ȡ�ö���������ŵ�������\n        #ע������������һ����my���ͱ���\n        my @trans=$2=~/[^@]+/g;\n        #����ֵΪ�����Ĺ�ϣ\n        $hash{$word}=\\@trans;\n    }\n}\nclose(In);\n}\n���Һ���\nsub Search{\n    my($word)=@_;\n#�ڹ�ϣ�����ҵ���\nif(defined $hash{$word}){\n    #�����ҵ���ȡ��ָ����������������\n    $pointer=$hash{$word};\n    #ͨ�����ã�����������Ա\n    foreach(@$pointer){\n        print &quot;$_\\n&quot;;\n    }\n}else{\n    print &quot;no found!\\n&quot;;\n}\n}\n��ʼ���ʱ�\nReadDict(&quot;dict.txt&quot;);while(1){\n#��ʾ��Ϣ\nprint &quot;Please input a word:(&#39;q&#39; for exit)\\n&quot;;\n#�ӱ�׼�����豸�еõ�����\n$input=&lt;stdin&gt;;\nchomp($input);\nif( $input eq &quot;q&quot;){\n    last;\n}\n#���ò��Һ���\nSearch($input);    \n}\n"},"chapter6/6_19.html":{"url":"chapter6/6_19.html","title":"6_19","body":"�������ܣ�ͳ��pos.txt�г��ֵĴʺʹ���\nʵ��˼�룺��ֵΪ��ϣ�Ĺ�ϣ��װ�ʺͶ�Ӧ����\nopen(In,&quot;pos.txt&quot;);\nwhile($Sent=){\n    chomp $Sent;\n    @Words=$Sent=~/(\\S+)\\/\\S+/g;\n#�õ�ÿ���ʵĴ���\n@POSs=$Sent=~/\\S+\\/(\\S+)/g;\n#�����ʺʹ��ԣ�����ֵΪ��ϣ�Ĺ�ϣ��%hash��\nfor($i=0; $i&lt;@Words; $i++) {\n    #���ҵ�ǰ���Ƿ��Ѿ���%hash��\n    if(defined $hash{$Words[$i]}) {\n        #ȡ��%hash��ֵ����ָ���ӹ�ϣ������\n        $refPOS=$hash{$Words[$i]};\n        #���ӹ�ϣ����һ������\n        ${$refPOS}{$POSs[$i]}=1;\n    }else{\n        #����һ��my���͵��ӹ�ϣ��ָ��������ϣ��������Ϊ%hash��ֵ\n        my %posHash=($POSs[$i],1);\n        $hash{$Words[$i]}=\\%posHash;\n    }\n  }\n}\nclose(In);\n����%hash\nforeach $word(sort keys %hash) {\n    print &quot;$word\\t&quot;;\n    $pPOS=$hash{$word};\n    foreach $pos(sort keys %{$pPOS}) {\n        print &quot;$pos;&quot;;\n    }\n    print &quot;\\n&quot;;\n}\n"},"chapter8/8_2.html":{"url":"chapter8/8_2.html","title":"8_2","body":"if (@ARGV != 3){\n    print &quot;usage:\\n mycode.pl �Cd|-e input1 input2\\n&quot;; \n    exit;\n}\nopen(In,&quot;$ARGV[1]&quot;);\nopen(Out,&quot;&gt;$ARGV[2]&quot;);\nwhile(){\n    chomp;\n    if ($ARGV[0] eq &quot;-d&quot; ){\n      print Out &quot;here is �Cd processing\\n&quot;;\n    } \nif ($ARGV[0] eq &quot;-e&quot; ){\n  print Out &quot;here is �Ce processing\\n&quot;;\n} \n}\nclose(In);\nclose(Out);\n"},"chapter8/8_3.html":{"url":"chapter8/8_3.html","title":"8_3","body":"$Input=;\nchomp($Input);\nprint stdout &quot;$Input\\n&quot;;\n"},"chapter8/8_4.html":{"url":"chapter8/8_4.html","title":"8_4","body":"while(&lt;&gt;){\n    chomp;\n    print