&lt;60 )\n{\n  print &quot;不及格&quot;;\n}\nelsif ( $Val&lt;80 )\n{\n  print &quot;及格&quot;;\n}\nelse\n{\n  print &quot;优秀&quot;;\n}\nprint &quot;\\n&quot;;\n\n"},"chapter2/2_8.html":{"url":"chapter2/2_8.html","title":"2_8 输入10以内的所有正整数","body":"$a=0;\nwhile ( $a&lt;10 ) {\n$a++;\nprint $a.&quot;\\t&quot;;\n}\n\n"},"chapter2/2_9.html":{"url":"chapter2/2_9.html","title":"2_9 有空语句的for循环","body":"for ( $i=1;$i&lt;10;) {\n    print &quot;$i\\t&quot;;\n    $i++;\n}\n\n"},"chapter2/2_10.html":{"url":"chapter2/2_10.html","title":"2_10 输入10以内的所有正整数","body":"for ( $i=1;$i&lt;10;$i++) {\n  print &quot;$i\\n&quot;;\n}\n\n"},"chapter2/2_11.html":{"url":"chapter2/2_11.html","title":"2_11 向屏幕输出一个等腰三角形","body":"print &quot;\\nPls input a num for printing:(1-10) \\n&quot;;\n$num = &lt;stdin&gt;;\nchomp ( $num );\nfor ($i=0;$i&lt;$num;$i++) {\n    for ($j=0;$j&lt;$num-$i;$j++) {\n        print &quot; &quot;;\n    }\n    for ($j=0;$j&lt;$i*2+1;$j++) {\n        print &quot;*&quot;;\n    }\n    print &quot;\\n&quot;;\n}\nprint &quot;\\n&quot;;\n\n"},"chapter2/2_12.html":{"url":"chapter2/2_12.html","title":"2_12 输出整数1~12，且没输出3个数字后换一行","body":"for($i=1;$i&lt;=12;$i++) {\n    if($i%3==0) {\n        print &quot;$i\\n&quot;;\n        next;\n    }\n    print &quot;$i &quot;;\n}\n\n"},"chapter2/2_13.html":{"url":"chapter2/2_13.html","title":"2_13 while(1)永真循环","body":"while(1) {\n    print &quot;pls input a string:\\&quot;q\\&quot; for exit\\n&quot;;\n    $str=&lt;stdin&gt;;\n    chomp($str);\n    if($str eq &quot;q&quot;) {\n        print &quot;THE END!\\n&quot;;\n        last;\n    }\n    print &quot;&gt;&gt;$str\\n&quot;;\n}\n\n"},"chapter3/3_1.html":{"url":"chapter3/3_1.html","title":"3_1 利用循环语句进行数组赋值","body":"for($i=0; $i&lt;5; $i++){\n    $array[$i]=2*$i;\n    print $array[$i].&quot;\\t&quot;;\n}\n\n"},"chapter3/3_2.html":{"url":"chapter3/3_2.html","title":"3_2 利用push()从尾部为数组@array添加成员","body":"@array=();\nfor($i=0; $i&lt;5; $i++){\n    push(@array,$i);\n}\n\n"},"chapter3/3_3.html":{"url":"chapter3/3_3.html","title":"3_3 利用unshift()从头部为数组@array添加成员","body":"@array=();\nfor($i=0; $i&lt;5; $i++){\n    unshift (@array,$i);\n}\n\n"},"chapter3/3_4.html":{"url":"chapter3/3_4.html","title":"3_4 利用push()一次添加一批成员","body":"@array1=(1,2);\n@array2=(&quot;a&quot;,&quot;b&quot;);\npush(@array1, @array2);\n\nforeach (@array1) {\n    print $_.&quot;\\t&quot;;\n}\n\n"},"chapter3/3_5.html":{"url":"chapter3/3_5.html","title":"3_5 访问数组成员","body":"@array=(1,2,3);\nprint $array[2];\n\n"},"chapter3/3_6.html":{"url":"chapter3/3_6.html","title":"3_6 获取数组成员个数","body":"@array=(&#39;a&#39;,&#39;b&#39;,&#39;c&#39;); \n$b=@array;\nprint $b;\n\n"},"chapter3/3_7.html":{"url":"chapter3/3_7.html","title":"3_7 利用数组下标和数组成员个数，对数组成员进行遍历","body":"@array=(1,2,3); \nfor($i=0; $i&lt;@array;$i++){\n    print &quot;$array[$i] &quot;;\n}\n\n"},"chapter3/3_8.html":{"url":"chapter3/3_8.html","title":"3_8 采用for语句实现数组成员遍历","body":"@array=(1,3.5,&quot;abc&quot;,&#39;cde&#39;); \nfor($i=0; $i&lt;@array;$i++)\n{\n    print &quot;$array[$i]\\t&quot;;\n}\n\n"},"chapter3/3_9.html":{"url":"chapter3/3_9.html","title":"3_9 采用foreach循环实现数组成员遍历","body":"@array=(&quot;hello&quot;,&quot;perl&quot;,&quot;world&quot;);\nforeach $word (@array)    {\n    if(length($word)&gt;4){\n        print &quot;$word &quot;;\n    }\n}\n\n"},"chapter3/3_10.html":{"url":"chapter3/3_10.html","title":"3_10 计算数值数组的平均值","body":"@array=(83,23,45,90,98);\nforeach (@array)    {\n    $sum+=$_;\n}\n$avg=$sum/@array;\nprint &quot;平均值：$avg&quot;;\n\n"},"chapter3/3_11.html":{"url":"chapter3/3_11.html","title":"3_11 一次获取数组的多个成员","body":"@array=(1,2,3,4,5);\n\n($first,$second)= @array;\nprint &quot;数组的第1个成员: $first\\n&quot;;\nprint &quot;数组的第2个成员: $second\\n&quot;;\n\n($first,$second,@subArray)= @array;\nprint &quot;$first $second &quot;;\n\nforeach $ele (@subArray){\n    print &quot;$ele &quot;;\n}\n\n"},"chapter3/3_12.html":{"url":"chapter3/3_12.html","title":"3_12 对字符串数组利用sort()进行排序","body":"@array=(&quot;a&quot;,&quot;bc&quot;,&quot;abc&quot;,&quot;bcd&quot;,&quot;c&quot;);\n@newArray=sort @array;\nforeach $ele(@newArray){\n    print $ele.&quot; &quot;;\n}\n\n"},"chapter3/3_13.html":{"url":"chapter3/3_13.html","title":"3_13 对数值数组利用sort()进行排序","body":"@array=(1,3,10,2,21);\n@newArray=sort @array;\nforeach $ele(@newArray){\n    print $ele.&quot; &quot;;\n}\n\n"},"chapter3/3_14.html":{"url":"chapter3/3_14.html","title":"3_14 对数组(@array)中的成员分别按数值大小进行升序或降序排列","body":"@array=(1,3,10,2,21);\n@newArray1=sort {$a &lt;=&gt; $b} @array;\nforeach $ele(@newArray1){\n    print $ele.&quot; &quot;;\n}\nprint &quot;\\n&quot;;\nforeach $ele(sort {$b &lt;=&gt; $a} @array){\n    print $ele.&quot; &quot;;\n}\n\n"},"chapter3/3_15.html":{"url":"chapter3/3_15.html","title":"3_15 数组成员的添加或删除","body":"@array=(&quot;a&quot;,&quot;bc&quot;,&quot;abc&quot;,&quot;bcd&quot;,&quot;c&quot;);\n$lastEle=pop(@array);\n$firstEle=shift(@array);\nunshift(@array,$lastEle);\npush(@array,$firstEle);\nprint &quot;$firstEle $lastEle\\n&quot;;\nforeach $ele(@array){\n    print $ele.&quot; &quot;;\n}\n\n"},"chapter3/3_16.html":{"url":"chapter3/3_16.html","title":"3_16 调用split()","body":"$aString=&quot;The deer did not trust us and kept their distance .&quot;;\n@array=split(&quot; &quot;,$aString);\nprint $aString. &quot;\\n&quot;;\nforeach $ele(sort @array){\n    print $ele.&quot; &quot;;\n}\n\n"},"chapter3/3_17.html":{"url":"chapter3/3_17.html","title":"3_17 调用join()","body":"@array=(&quot;The&quot;,&quot;deer&quot;,&quot;did&quot;,&quot;not&quot;,&quot;trust&quot;,&quot;us&quot;,\n                &quot;and&quot;,&quot;kept&quot;,&quot;their&quot;,&quot;distance&quot;,&quot;.&quot;);\n$aString=join(&quot; &quot;,@array);\nprint $aString;\n\n"},"chapter3/3_18.html":{"url":"chapter3/3_18.html","title":"3_18 利用数组输出指定字符串","body":"$line=&quot;The most precious possession that \n             ever comes to a man in this world is a woman&#39;s heart .&quot;;\n#以空格为分隔符，将$line分割成若干子串，并将每个子串存入数组（@array）\n@array=split(&quot; &quot;,$line); \n\nforeach $word (sort @array)    {\n    #将长度大于5的子串放入数组（@words）\n    if ( length($word)&gt;5 ){  \n        push(@words,$word);\n    }\n}\n\nprint &quot;word&#39;s length is more than 5:\\n&quot;;\n#有序地遍历数组成员\nforeach (sort @words)    {  \n    print &quot;$_\\t&quot;;\n}\n\n"},"chapter3/3_19.html":{"url":"chapter3/3_19.html","title":"3_19 访问哈希成员","body":"%hash = (&quot;Michael&quot;,21,&quot;Candy&quot;,30,&quot;Ethan&quot;,18);\n$age = $hash{&quot;Michael&quot;};\nprint &quot;哈希成员访问：\\n&quot;;\nprint &quot;Michael: $age\\n&quot;;\n\n"},"chapter3/3_20.html":{"url":"chapter3/3_20.html","title":"3_20 关键字数组排序","body":"%hash = (&quot;Michael&quot;,21,&quot;Candy&quot;,30,&quot;Ethan&quot;,18);\n@arr=keys(%hash);\nforeach (@arr){\n    print $_.&quot; &quot;;    \n}\n\n"},"chapter3/3_21.html":{"url":"chapter3/3_21.html","title":"3_21 哈希成员遍历(1)","body":"%Ages = (&quot;Maris&quot;, 61, &quot;Aaron&quot;, 75, &quot;Young&quot;, 51);\n@names=keys %Ages;\nforeach $Name (@names){\n    print &quot;$Name:$Ages{$Name}\\n&quot;;\n}\n\n"},"chapter3/3_22.html":{"url":"chapter3/3_22.html","title":"3_22 哈希成员遍历(2)","body":"%Ages = (&quot;Maris&quot;, 61, &quot;Aaron&quot;, 75, &quot;Young&quot;, 51);\nforeach $Name (keys %Ages){\n    print &quot;$Name:$Ages{$Name}\\n&quot;;\n}\n\n"},"chapter3/3_23.html":{"url":"chapter3/3_23.html","title":"3_23 利用sort()实现对哈希成员的有序遍历","body":"%Ages = (&quot;Maris&quot;, 61, &quot;Aaron&quot;, 75, &quot;Young&quot;, 51);\nforeach $Name (sort keys %Ages){\n    print &quot;$Name    $Ages{$Name}\\n&quot;;\n}\n\n"},"chapter3/3_24.html":{"url":"chapter3/3_24.html","title":"3_24 利用按照值大小排序的方式进行哈希成员遍历","body":"%Ages = (&quot;Maris&quot;, 61, &quot;Aaron&quot;, 75, &quot;Young&quot;, 51);\nforeach $aName (sort{$Ages{$a}&lt;=&gt;$Ages{$b}} keys %Ages){\n    print &quot;$aName    $Ages{$aName}\\n&quot;;\n}\n\n"},"chapter3/3_25.html":{"url":"chapter3/3_25.html","title":"3_25 查找一个哈希成员","body":"%Ages = (&quot;Maris&quot;, 61, &quot;Aaron&quot;, 75, &quot;Young&quot;, 51);\nif(defined $Ages{&quot;Mark&quot;}){\n    $Ages{&quot;Mark&quot;}++;\n}\nelse{\n    $Ages{&quot;Mark&quot;}=20;\n}\nforeach $aName (sort {$Ages{$a} &lt;=&gt; $Ages{$b} } keys %Ages){\n    print &quot;$aName\\t$Ages{$aName}\\n&quot;;\n}\n\n"},"chapter3/3_26.html":{"url":"chapter3/3_26.html","title":"3_26 从%Ages中删除以\"Aaron\"为关键字的成员","body":"%Ages = (&quot;Maris&quot;, 61, &quot;Aaron&quot;, 75, &quot;Young&quot;, 51);\ndelete $Ages{&quot;Aaron&quot;};\nforeach $Name (sort keys %Ages){\n    print &quot;$Name    $Ages{$Name}\\n&quot;;\n}\n\n"},"chapter3/3_27.html":{"url":"chapter3/3_27.html","title":"3_27 利用数组建立哈希变量，并按指定格式输出；查找哈希变量并输出指定字符串","body":"@name=(&quot;Lingkar&quot;,&quot;Klara&quot;,&quot;Aurora&quot;);\n@ages=(26,27,24);\n\n#@name和@ages元素个数相同，$i&lt;@name来控制循环\nfor($i=0;$i&lt;@name;$i++)    {\n    #姓名为key，年龄为value\n    $hash{$name[$i]}=$ages[$i];    \n}\n\n#遍历哈希\nforeach $member(sort keys %hash)    {\n    print &quot;$member is $hash{$member} years old.\\n&quot;;\n}\n\n#查找一个姓名\nif(defined $hash{&quot;Babara&quot;}){\n    print &quot;%hash{\\&quot;Babara\\&quot;}\\n&quot;;\n}else{\n    print &quot;Not Found!\\n&quot;;\n}\n\n"},"chapter3/3_28.html":{"url":"chapter3/3_28.html","title":"3_28 打开文件并读取文件内容，将文件内容一行一行输出到屏幕上","body":"#一个常用读文件的样例\nopen(In,&quot;read_file.txt&quot;);\nwhile( $line=&lt;In&gt; ){\n    chomp($line);\n    print $line.&quot;\\n&quot;;    #将文件内容一行一行输出到屏幕上\n}\nclose(In);\n\n"},"chapter3/3_29.html":{"url":"chapter3/3_29.html","title":"3_29 采用一次读取并储存到数组的方式将指定文件内容逐行输出到屏幕上","body":"open(In,&quot;read_file.txt&quot;) || die(&quot;error in open!&quot;);\n@lines=&lt;In&gt;;\nchomp(@lines);\nclose(In);\n\nforeach $Line (@lines){\n    print &quot;$Line\\n&quot;;\n}\n\n"},"chapter3/3_30.html":{"url":"chapter3/3_30.html","title":"3_30 从屏幕读取用户输入的一行数据","body":"#从标准输入中读取一个字符串\n$line=&lt;stdin&gt;;\nprint $line;\n\n"},"chapter3/3_31.html":{"url":"chapter3/3_31.html","title":"3_31 将例3-28中文件read-file.txt中内容复制到另外一个文件out.txt中","body":"open(In,&quot;read_file.txt&quot;);\nopen(Out,&quot;&gt;out.txt&quot;);\nwhile($lines=&lt;In&gt;){\n    chomp($lines);\n    print Out &quot;$lines\\n&quot;;\n}\nclose(In);\nclose(Out);\n\n"},"chapter3/3_32.html":{"url":"chapter3/3_32.html","title":"3_32 取得指定目录下的所有文件及目录信息，并将这些信息输出到屏幕","body":"#输出d:\\perl\\目录下所有文件\nopendir(Dir,&quot;D:\\\\perl\\\\&quot;);    \n@File=readdir(Dir);\nforeach $file_name(@File){\n    print &quot;$file_name\\n&quot;;\n}\nclose(Dir);\n\n"},"chapter3/3_33.html":{"url":"chapter3/3_33.html","title":"3_33 读取当前目录下的所有文本文件，并将这些文件的内容合并到一个文件中","body":"use Cwd;    #为了保证平台的兼容性，多使用Cwd来获取当前目录；\n$CurrentDir=cwd();  #变量$Current获取当前目录\nopendir(Dir,$CurrentDir) || die(&quot;error in opendir!&quot;);\n@files=readdir(Dir);\nclose(Dir);\n\nopen(Out,&quot;&gt;out.txt&quot;);\nforeach(@files){\n    if(index($_,&quot;.txt&quot;) == length($_)-4){\n        open(In,$_);\n        while(&lt;In&gt;){    \n            chomp;\n            print Out $_.&quot;\\n&quot;;\n        }\n        close(In);\n    }\n}\nclose(Out);\n\n"},"chapter3/3_34.html":{"url":"chapter3/3_34.html","title":"3_34 合并文本a.txt和b.txt的内容，作为新文本c.txt的内容","body":"open (Out,&quot;&gt;c.txt&quot;);\n\nopen(In,&quot;a.txt&quot;);\nwhile( $Line=&lt;In&gt;) {\n    chomp($Line);\n    print Out &quot;$Line\\n&quot;;\n}\nclose(In);\n\nopen (In,&quot;b.txt&quot;);\nwhile ( $Line=&lt;In&gt;) {\n    chomp($Line);\n    print Out &quot;$Line\\n&quot;;\n}\nclose(In);\n\nclose(Out);\n\n"},"chapter3/3_35.html":{"url":"chapter3/3_35.html","title":"3_35 里3-34的简化形式","body":"open (Out,&quot;&gt;c.txt&quot;);\n@File=(&quot;a.txt&quot;,&quot;b.txt&quot;);\nforeach $file(@Files){\n\n    open(In,&quot;$file&quot;);\n    while ($Line=&lt;In&gt;) {\n        chomp($Line);\n        print Out &quot;$Line\\n&quot;;\n    }\n    close(In);\n}\nclose(Out);\n\n"},"chapter3/3_36.html":{"url":"chapter3/3_36.html","title":"3_36 例3-34的函数形式","body":"open (Out,&quot;&gt;c.txt&quot;);\n@File=(&quot;a.txt&quot;,&quot;b.txt&quot;);\nforeach $file(@Files){\n  Process($file,Out);\n}\nclose(Out);\n\nsub Process\n{\n    my($file,$Handle)=@_;\n    open(In,&quot;$file&quot;);\n    while ($Line=&lt;In&gt;) {\n            chomp($Line);\n            print $Handle &quot;$Line\\n&quot;;\n        }\n        close(In);\n}\n\n"},"chapter3/3_37.html":{"url":"chapter3/3_37.html","title":"3_37 定义函数HelloOrBye(),并连续3次调用函数","body":"for ($i=1; $i&lt;=3;$i++) {\n    print &quot;第$i次调用HelloOrBye:&quot;;\n    HelloOrBye();\n}\n\nsub HelloOrBye(){\n    my $str1=&quot;hello,world!\\n&quot;;\n    my $str2=&quot;good bye!\\n&quot;;\n\n    my $n=int(rand(2));\n    if($n) {\n      print $str1;\n    }\n    else{\n      print $str2;\n    }\n}\n\n"},"chapter3/3_38.html":{"url":"chapter3/3_38.html","title":"3_38 变量作用域示例","body":"$a=1;    #具有全局作用域的全局变量（$a）\nfunc();\nprint $a.&quot; &quot;;\n\nsub func(){\n    my $a=2;    #具有函数作用域的局部变量（$a）\n    {\n        my $a=3;    #具有代码块作用域的局部变量（$a）\n        print $a.&quot; &quot;;\n    }\n    print $a.&quot; &quot;;\n}\n\n"},"chapter3/3_39.html":{"url":"chapter3/3_39.html","title":"3_39 调用多个子函数","body":"func1();\nfunc2();\n\nsub func1(){\n    my $a=&quot;Hello,world!\\n&quot;;\n    print $a;\n}\nsub func2(){\n    my $a=&quot;It&#39;s an example.\\n&quot;;\n    print $a;\n}\n\n"},"chapter3/3_40.html":{"url":"chapter3/3_40.html","title":"3_40 使用全局变量想函数传递参数","body":"$val=&quot;string1&quot;;\nfunc();\n$val=&quot;string2&quot;;\nfunc();\nsub func{\n    print &quot;this is $val\\n&quot;;\n}\n\n"},"chapter3/3_41.html":{"url":"chapter3/3_41.html","title":"3_41 利用默认参数列表变量向函数传递参数","body":"print &quot;\\n&quot;;\nfunc(&quot;string1&quot;);\nfunc(&quot;string2&quot;);\nsub func{\n    my ($val)=@_;\n    print &quot;this is $val\\n&quot;;\n}\nprint &quot;\\n&quot;;\n\n"},"chapter3/3_42.html":{"url":"chapter3/3_42.html","title":"3_42 利用默认参数列表变量实现例3-34的任务","body":"open(Out,&quot;&gt;c.txt&quot;);\nMergeFile(&quot;a.txt&quot;,Out);\nMergeFile(&quot;b.txt&quot;,Out);\nclose(Out);\n\nsub MergeFile(){\n    my ($filename, $fp)=@_;\n    open(In,$filename);\n    while(&lt;In&gt;){\n        chomp;\n        print $fp &quot;$_\\n&quot;;\n    }\n    close(In);\n}\n\n"},"chapter3/3_43.html":{"url":"chapter3/3_43.html","title":"3_43 使用return语句实现函数返回值","body":"print hello(&quot;world&quot;);\nsub hello(){\n    my ($str)=@_;\n    $str=&quot;hello $str!\\n&quot;;\n    return $str;\n}\n\n"},"chapter5/5_1.html":{"url":"chapter5/5_1.html","title":"5_1","body":"#�������ܣ���$line�в����Ƿ����Ӵ���hello��\n$line=&quot;hello world&quot;;\nif ($line =~/hello/) { \n    print &quot;Got it!&quot;; \n}\n\n"},"chapter5/5_2.html":{"url":"chapter5/5_2.html","title":"5_2","body":"�������ܣ���$line�в��������Ӵ�\nʵ��˼�룺��������ƥ����1�����ֵķ���ģʽ�����󳤶��Ӵ�\n$line=&quot;hello 123456 world 789&quot;;\nif ($line =~/\\d+/) { \n    print &quot;$&amp;&quot;; \n}\n"},"chapter5/5_3.html":{"url":"chapter5/5_3.html","title":"5_3","body":"$string=&quot;aa1bb&quot;;\nif ( $string=~/a[0-9]b/ ){\n    print &quot;$&amp;\\n&quot;;\n}\n"},"chapter5/5_4.html":{"url":"chapter5/5_4.html","title":"5_4","body":"@array=(&quot;abc&quot;,&quot;abbc&quot;,&quot;1bcd&quot;,&quot;dbc&quot;);\nforeach $value(@array){\n    if($value=~/[ab12]bc/){\n        print &quot;$value:$&amp;\\n&quot;; \n    }\n    else{\n        print &quot;$value:no match\\n&quot;;\n    }\n}\n"},"chapter5/5_5.html":{"url":"chapter5/5_5.html","title":"5_5","body":"$string=&quot;aa123567bb&quot;;\nif($string=~/ab+/){\n    print $&amp;;\n}\n"},"chapter5/5_6.html":{"url":"chapter5/5_6.html","title":"5_6","body":"�������ܣ������飨@array�����ҳ��������ֵĳ�Ա\n@array=(&quot;abc&quot;,&quot;abbc&quot;,&quot;1bcd&quot;,&quot;dbc&quot;);\nforeach $value(@array){\n������@array�ĳ�Ա��$value��Ϊ����������ֻҪ$value��\n���������ַ�������ƥ���ɹ���������T��������������F��\nif($value=~/[^0-9]/){\n    print &quot;T\\t&quot;; \n}\nelse{\n    print &quot;F\\t&quot;;\n}\n}\n"},"chapter5/5_7.html":{"url":"chapter5/5_7.html","title":"5_7","body":"�������ܣ��ҳ����飨@array�����ԡ�ab����ͷ���ԡ�c����β�ĳ���Ϊ4�ĳ�Ա\n@array=(&quot;abac&quot;,&quot;abbc&quot;,&quot;ab1c&quot;,&quot;ab\\nc&quot;);\nforeach $value (@array){\nif($value=~/ab.c/){\n    print &quot;T\\t&quot;; \n}\nelse{\n    print &quot;F\\t&quot;;\n}\n}\n"},"chapter5/5_8.html":{"url":"chapter5/5_8.html","title":"5_8","body":"$string=&quot;[123 abc]&quot;;\nif($string=~/\\[\\]+/){\n    print $&amp;;\n}\n"},"chapter5/5_9.html":{"url":"chapter5/5_9.html","title":"5_9","body":"�������ܣ��ҳ����飨@array�����ԡ�b����ͷ���ԡ�a����β�ĳ���Ϊ2��3�ĳ�Ա\n@array=(&quot;aab&quot;,&quot;bab&quot;,&quot;baaaba&quot;,&quot;baaaaaaaab&quot;);\nforeach $value(@array){\n    if( $value=~/b.?a/){\n        print &quot;T:$value\\t&quot;;\n    }else{\n        print &quot;F\\t&quot;;\n    }\n}\n"},"chapter5/5_10.html":{"url":"chapter5/5_10.html","title":"5_10","body":"�������ܣ��ҳ����飨@array�����ҳ����С�b��֮�����С�a���ĳ�Ա\n@array=(&quot;aab&quot;,&quot;bab&quot;,&quot;baaaba&quot;,&quot;baaaaaaaab&quot;);\nforeach $value(@array){\n    if( $value=~/b.*a/){\n        print &quot;T:$value\\t&quot;;\n    }else{\n        print &quot;F\\t&quot;;\n    }\n}\n"},"chapter5/5_11.html":{"url":"chapter5/5_11.html","title":"5_11","body":"�������ܣ������飨@array�����ҳ���a�����ٳ���2�Σ���������4�Σ�������\n����1����b���ĳ�Ա\n@array=(&quot;aab&quot;,&quot;bab&quot;,&quot;baaab&quot;,&quot;baaaaaaaab&quot;);\nforeach $value(@array){\n    if($value=~/a{2,4}b/){\n        print &quot;T:$value\\t&quot;; \n    }\n    else{\n        print &quot;F\\t&quot;;\n    }\n}\n"},"chapter5/5_12.html":{"url":"chapter5/5_12.html","title":"5_12","body":"�������ܣ������飨@array�����ҳ��ԡ�hello����ͷ�ĳ�Ա\n@array=(&quot;hello&quot;,&quot;Hi,hello&quot;,&quot; hello&quot;,&quot;ab\\nc&quot;);\nforeach(@array){\n    if(/^hello/){\n        print &quot;T\\t&quot;; \n    }\n    else{\n        print &quot;F\\t&quot;;\n    }\n}\n"},"chapter5/5_13.html":{"url":"chapter5/5_13.html","title":"5_13","body":"�������ܣ������飨@array�����ҳ��ԡ�good bye����β�ĳ�Ա\n@array=(&quot;hello&quot;,&quot;Hi,good bye&quot;,&quot;good bye!&quot;,&quot;good bye bye&quot;);\nforeach(@array){\n    if(/good bye$/){\n        print &quot;T\\t&quot;; \n    }\n    else{\n        print &quot;F\\t&quot;;\n    }\n}\n"},"chapter5/5_14.html":{"url":"chapter5/5_14.html","title":"5_14","body":"$str=&quot;abbcd&quot;;\nif($str=~/abc|bcd/){\n    print $&amp;;\n}\n"},"chapter5/5_15.html":{"url":"chapter5/5_15.html","title":"5_15","body":"�������ܣ������飨@array�����ҳ��ԡ�ab����ͷ�ԡ�c����β�ĳ���Ϊ4�ĳ�Ա\n�����ԡ�a����ͷ�ԡ�d����β�ĳ���Ϊ3�ĳ�Ա\n@array=(&quot;abac&quot;,&quot;abbc&quot;,&quot;ab1c&quot;,&quot;abd&quot;,&quot;abcd&quot;);\nforeach $value (@array){\n    if($value=~/ab.c|a.d/){\n        print &quot;T\\t&quot;; \n    }\n    else{\n        print &quot;F\\t&quot;;\n    }\n}\n"},"chapter5/5_16.html":{"url":"chapter5/5_16.html","title":"5_16","body":"�������ܣ���$line���ҳ�����������������a�����ԡ�b���������Ӵ�\n����������\n$line=&quot;aaababaaaaaaab&quot;;\n@array=$line=~/a*b/g;foreach (@array){    print $_.&quot;\\t&quot;;}        \n"},"chapter5/5_17.html":{"url":"chapter5/5_17.html","title":"5_17","body":"�������ܣ��ж�$line���Ƿ����ڡ�hello�������ִ�Сд��\n$line=&quot;Hello,world&quot;;\n$line=~/hello/i;\nprint &quot;$&amp;&quot;;\n"},"chapter5/5_18.html":{"url":"chapter5/5_18.html","title":"5_18","body":"�������ܣ��ҳ�$line�к��Դ�Сд�ĺ���������������a��\n���ԡ�b���������Ӵ�\n$line=&quot;aaabAbaAAaaaab&quot;;\n@array=$line=~/a*b/gi;\n�������飨@array��\nforeach(@array){\n    print $_.&quot;\\t&quot;;\n}\n"},"chapter5/5_19.html":{"url":"chapter5/5_19.html","title":"5_19","body":"�������ܣ��ں��л��з���$line�У��ҳ��ԡ�H����ͷ�ԡ�d����β��\n�м京�������ַ����Ӵ�\n$line=&quot;Hello\\nworld&quot;;\n$line=~/H.*d/s;\nprint $&amp;;\n"},"chapter5/5_20.html":{"url":"chapter5/5_20.html","title":"5_20","body":"�������ܣ���$line�еġ�world���滻�ɡ�everyone��\n$line=&quot;Hello, world&quot;;\n$line =~s/world/everyone/; \nprint &quot;$line&quot;; \n"},"chapter5/5_21.html":{"url":"chapter5/5_21.html","title":"5_21","body":"�������ܣ���I Love You 9������ĸ���������滻\n$line=&quot;I Love You 9&quot;;\n��$line��